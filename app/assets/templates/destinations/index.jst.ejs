<div class="search-box col-md-4 col-md-offset-4">
  <h4>Where are you going?</h4>
  <input type="text"
    class='search-box'
    name="destination[name]">
  <div class="search-results">
  </div>
  <% places.each( function(place){ %>
    <a href="#/destinations/<%=place.escape('id')%>">
      <%=place.escape('name')%>
    </a>
  <%  }) %>
</div>
